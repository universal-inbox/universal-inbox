{
  "db_name": "PostgreSQL",
  "query": "\n                UPDATE\n                  notification\n                SET\n                  status = $1::notification_status\n                WHERE\n                  NOT source_id = ANY($2)\n                  AND kind = $3\n                  AND (status::TEXT = 'Read' OR status::TEXT = 'Unread')\n                RETURNING\n                  id,\n                  title,\n                  status as \"status: _\",\n                  source_id,\n                  source_html_url,\n                  metadata as \"metadata: Json<NotificationMetadata>\",\n                  updated_at,\n                  last_read_at,\n                  snoozed_until,\n                  user_id,\n                  task_id\n            ",
