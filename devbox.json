{
  "packages": {
    "sqlx-cli":       "0.7.4",
    "trunk":          "latest",
    "pre-commit":     "latest",
    "rustup":         "latest",
    "cargo-audit":    "latest",
    "cargo-expand":   "latest",
    "cargo-nextest":  "latest",
    "cargo-outdated": "latest",
    "just":           "latest",
    "watchexec":      "latest",
    "postgresql_15":  "latest",
    "redis":          "latest",
    "iconv":          "latest",
    "nodejs":         "latest",
    "pkg-config":     "latest",
    "taplo":          "latest",
    "darwin.apple_sdk.frameworks.Foundation": {
      "platforms": [
        "x86_64-darwin",
        "aarch64-darwin"
      ]
    },
    "lld": {
      "version": "latest",
      "platforms": [
        "x86_64-darwin",
        "aarch64-darwin"
      ]
    },
    "fzf":              "latest",
    "process-compose":  "latest",
    "binaryen":         "latest",
    "cargo-chef":       "latest",
    "cargo-binstall":   "latest",
    "openssl.dev":      "",
    "wasm-bindgen-cli": "0.2.92",
    "cargo-machete":    "latest"
  },
  "shell": {
    "init_hook": [
      "just init-db",
      "just install-rust-toolchain",
      "just install-rust-tools"
    ]
  },
  "env": {
    "TARGET_CC":              "cc",
    "RUST_TOOLCHAIN_VERSION": "1.79"
  }
}
